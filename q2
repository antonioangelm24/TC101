#include <iostream>
#include <cmath>
using namespace std;

int superpower(long a, long b){
	long x;
	int n;
	n = 1;
	x = a;
	do{
		x = (x * a);
		n++;
		
	}while (n<b);
	return x;
	
		
}

int main(){
	long a, b;
	cout << "Give the first number ";
	cin >> a;
	cout << "Give me the second number ";
	cin >> b;
	cout << superpower(a,b);
	cout << endl;
	return 0;
}
